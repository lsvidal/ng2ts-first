{
  "name": "Final-Project",
  "version": "1.0.0",
  "authors": [
    "Leonardo Vidal <leonardo73vidal@gmail.com>"
  ],
  "description": "Projeto final do curso de Typescript.",
  "moduleType": [
    "system"
  ],
  "keywords": [
    "typescript"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "~3.3.4",
    "traceur-runtime": "~0.0.89",
    "es6-module-loader": "~0.16.6",
    "systemjs": "systemjs/systemjs#~0.16.11"
  },
  "overrides": {
    "*": {      
      "normalize": {
        ".": ["*.js", "*.map"]
      }
    },
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.css.map",
        "dist/js/bootstrap.js",
        "dist/fonts/*"
      ],
      "normalize": {
          "font": [
            "*.ttf",
            "*.woff",
            "*.woff2",
            "*.eot",
            "*.svg"
          ],
          "css": [
            "*.css.map"
          ]
        }
    },
    "systemjs": {
      "main": [
        "dist/system.js",
        "dist/system.js.map"
      ],
      "normalize": {
        "js": "*.map"
      }
    },
    "es6-module-loader": {
      "main": "dist/es6-module-loader-sans-promises.js*",
      "normalize": {
        "js": "*.map"
      }
    }
  }
}
